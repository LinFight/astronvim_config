{
  "for-heex": {
    "prefix": "forstream",
    "body": [
      "<div :for={ {dom_id, $1} <- @$2 } id={\"$1-#{dom_id}\"} class=\"$3\">",
      "",
      "</div>"
    ]
  },
  "=": {
    "prefix": "=",
    "body": [
      "<%= $1 %>"
    ]
  },
  "if": {
    "prefix": "if",
    "body": [
      "<div :if{ $1 } class=\"$2\"",
      "",
      "</div>"
    ]
  }
}
